.mention {
    display: inline-block;
    border-radius: 3px;
    text-decoration: none;
    position: relative;
    font-family: sans-serif;
    font-weight: 100;
    padding: 3px;
    padding-right: 7px;
}

.icon {
    width: 15px;
    display: inline-block;
    position: absolute;
    top: 4px;
    left: 5px;
}

.prefixIcon {
    max-height: 15px;
    display: inline-block;
    position: absolute;
    top: 4px;
    left: 15px;
}

.suffixIcon {
    width: 15px;
    display: inline-block;
    position: absolute;
    top: 4px;
    right: 5px;
}

.text {
    padding-left: 20px;
    white-space: pre-wrap;
    max-height: 20px;
}