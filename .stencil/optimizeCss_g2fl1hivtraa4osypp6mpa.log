.mention {
    border-radius: 3px;
    text-decoration: none;
    position: relative;
    font-family: sans-serif;
    font-weight: 100;
    padding: 3px;

    display: -ms-flexbox;

    display: flex;
    -ms-flex-wrap: nowrap;
    flex-wrap: nowrap;
    -ms-flex-direction: row;
    flex-direction: row;
}

.icon {
    width: 15px;
    display: inline-block;
    top: 4px;
    left: 5px;
}

.prefixIcon {
    max-height: 15px;
    display: inline-block;
    top: 4px;
    left: 5px;
}

.suffixIcon {
    width: 15px;
    display: inline-block;
    top: 4px;
    right: 5px;
}

.text {
    text-overflow: ellipsis;
    padding-left: 20px;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-break: break-all;
    -ms-flex-positive: 1;
    flex-grow: 1;
}