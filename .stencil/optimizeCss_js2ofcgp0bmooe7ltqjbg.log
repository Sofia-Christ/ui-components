.mention {
    display: inline-block;
    border-radius: 3px;
    text-decoration: none;
    position: relative;
    font-family: sans-serif;
    font-weight: 100;
    padding: 3px;
    padding-right: 7px;
}

.icon {
    width: 15px;
    display: inline-block;
    position: absolute;
    top: 2px;
    left: 2px;
}

.text {
    padding-left: 20px;
}