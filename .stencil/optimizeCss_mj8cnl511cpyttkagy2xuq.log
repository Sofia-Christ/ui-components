.avatar {
    width: 100%;
    height: 100%;
    border-radius: 50%;
    display: block;
}

.avatar-container {
    width: 100%;
    display: inline-block;
    background-color: blue;
    position: relative;
}

.icon {
    max-width: 40%;
    max-height: 40%;
    position: relative;
    border-radius: 20%;
}

.top {
    float: top;;

}

.bottom {
    float: bottom;;

}

.left {
    float: left;;

}

.right {
    float: right;
}